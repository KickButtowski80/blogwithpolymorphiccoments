<%#= form_with(model: [commentable, Comment.new],local: true) do |form| %>
<%= form_with model: commentable, local: true do |form| %>
  <p >
    <%= form.label :commenter %><br>
    <%= form.text_field :commenter, contenteditable: "true"%>
  </p>
  <p contenteditable="true">
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>